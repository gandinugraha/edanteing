<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>BarangModel</servlet-name>
        <servlet-class>com.csa.model.BarangModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>koneksi</servlet-name>
        <servlet-class>com.csa.model.Koneksi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BarangController</servlet-name>
        <servlet-class>com.csa.controller.BarangController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminModel</servlet-name>
        <servlet-class>com.csa.model.AdminModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminController</servlet-name>
        <servlet-class>com.csa.controller.AdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PembeliController</servlet-name>
        <servlet-class>com.csa.controller.PembeliController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PembeliModel</servlet-name>
        <servlet-class>com.csa.model.PembeliModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginAdmin</servlet-name>
        <servlet-class>com.csa.model.LoginAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginPembeli</servlet-name>
        <servlet-class>com.csa.model.LoginPembeli</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cart</servlet-name>
        <servlet-class>com.csa.model.Cart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddtoCart</servlet-name>
        <servlet-class>com.csa.controller.AddtoCart</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BarangModel</servlet-name>
        <url-pattern>/BarangModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>koneksi</servlet-name>
        <url-pattern>/koneksi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BarangController</servlet-name>
        <url-pattern>/BarangController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminModel</servlet-name>
        <url-pattern>/AdminModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminController</servlet-name>
        <url-pattern>/AdminController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PembeliController</servlet-name>
        <url-pattern>/PembeliController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PembeliModel</servlet-name>
        <url-pattern>/PembeliModel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginAdmin</servlet-name>
        <url-pattern>/LoginAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginPembeli</servlet-name>
        <url-pattern>/LoginPembeli</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cart</servlet-name>
        <url-pattern>/cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddtoCart</servlet-name>
        <url-pattern>/AddtoCart</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>halaman_utama.jsp</welcome-file>
    </welcome-file-list>
</web-app>
